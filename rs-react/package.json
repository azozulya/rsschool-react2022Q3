{
  "name": "rs-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rolling-scopes-school.github.io/azozulya-REACT2022Q3/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.60",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .tsx --fix",
    "prettier": "prettier --write src/**/*.tsx",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "cd .. && husky install rs-react/.husky"
  },
  "lint-staged": {
    "*.(ts|tsx)": ["npm run lint"]     
  },
  "husky": {    
   "hooks": {      
     "pre-commit": "lint-staged"
   }  
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.4.1"
  }
}
